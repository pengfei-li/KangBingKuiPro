/* pages/github/github.wxss */
.page-content{
    padding-top: 0;
}
.item-name{
    font-size: 18px;
    font-weight:bold;
}
.item-desc{
    font-size: 14px;
}
.item-info{
    margin-top: 10rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 16px;
    color: #2c2c2c;
}
.userInfo,
.start{
    line-height: 60rpx;
    vertical-align: middle;
}
.icon{
    width: 50rpx;
    height: 50rpx;
    border-radius: 30rpx;
    vertical-align: middle;
}
.icon-start{
    margin-right: 10rpx;
}
.start{
    text-align: center;
    justify-content:center;
    display: flex;
}
.language{
    text-align: right;
}
/*不同类别的颜色*/
.JavaScript{
    color:#42afff;
}
.CSS{
    color: #3ee4bf;
}
.HTML{
    color: #ffac45;
}